<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Express Myself</title>
</head>
<body>
  <h3><%= greeting %></h3>
  <h4>My skills include</h4>
  <ul>
    <% skills.forEach(function(skill) { %>
      <li> <%= skill %> </li>
    <% }) %>
  </ul>
  <h4>My hobbies</h4>
  <ul>
    <% hobbies.forEach(function(hobby) { %>
      <li> <%= hobby %> </li>
    <% }) %>
  </ul>
</body>
</html>
